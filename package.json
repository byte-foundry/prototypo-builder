    "eslint-config-recommended": "^1.5.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-mocha": "^4.5.1",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-react-native": "^2.0.0",
    "file-loader": "^0.9.0",
    "glob": "^7.0.0",
