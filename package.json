{
  "name": "prototypoBuilder",
  "private": true,
  "version": "1.0.0",
  "description": "Parametric glyph builder for Prototypo - WORK IN PROGRESS",
  "main": "",
  "scripts": {
    "start": "node server.js --env=dev",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "posttest": "npm run lint",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "dist": "npm run copy & webpack --env=dist",
    "lint": "eslint ./src ./__tests__",
    "copy": "npm run copy:flat && npm run copy:up",
    "copy:up": "copyfiles -u 1 ./src/images/*.png ./dist",
    "copy:flat": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "clean": "rimraf dist/*",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags"
  },
  "jest": {
    "testRegex": "/__tests__/.*Test\\.(js|jsx)$",
    "moduleNameMapper": {
      "^config$": "<rootDir>/src/config/test.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|sass)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository": "",
  "keywords": [],
  "author": "@louis_remi",
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.0",
    "babel-plugin-resolver": "1.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-root-import": "^4.1.3",
    "bower-webpack-plugin": "^0.1.9",
    "copyfiles": "^1.0.0",
    "core-js": "^2.0.0",
    "css-loader": "^0.25.0",
    "deep-freeze": "0.0.1",
    "eslint": "^3.12.2",
    "eslint-import-resolver-babel-root-import": "0.0.2",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-react-native": "^2.0.0",
    "file-loader": "^0.9.0",
    "glob": "^7.0.0",
    "jest": "^17.0.3",
    "minimist": "^1.2.0",
    "node-sass": "^3.10.0",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "postcss": "^5.0.11",
    "postcss-loader": "^0.13.0",
    "react-addons-test-utils": "^15.0.0",
    "react-hot-loader": "^3.0.0-beta.4",
    "rimraf": "^2.4.3",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.0",
    "surge": "^0.18.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "bezier-js": "github:louisremi/bezierjs#feat/FP-version",
    "classnames": "^2.2.4",
    "deptree": "^0.1.0",
    "memoize-immutable": "^2.0.0",
    "normalize.css": "^4.0.0",
    "ramda": "^0.22.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^5.0.0",
    "redux": "^3.6.0",
    "reselect": "^2.5.1"
  }
}
